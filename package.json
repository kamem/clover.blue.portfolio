{
    "name": "clover.blue",
    "version": "1.0.0",
    "description": "",
    "main": "src/app.js",
    "scripts": {
        "postinstall": "npm run build",
        "build": "NODE_ENV=production webpack --config webpack/webpack.app.config.js",
        "start-dev": "NODE_ENV=development PORT=1341 node -r babel-register webpack/webpack-dev-server.express.js",
        "start-dev:lint": "NODE_ENV=development PORT=1341 LINT=true node -r babel-register webpack/webpack-dev-server.express.js",
        "start-server:prd": "NODE_ENV=production PORT=3141 nodemon -r babel-register --watch server -e js,ejs bin/www --exec 'npm run lint:server && node'",
        "start-server:dev": "NODE_ENV=development PORT=3141 nodemon -r babel-register --watch server -e js,ejs bin/www --exec 'npm run lint:server && node'",
        "lint:src": "eslint ./src --fix",
        "lint:server": "eslint ./server/api/ --fix",
        "browser-sync": "PORT=3141 node -r babel-register dev/browser-sync.js",
        "postcss": "postcss src/css/app.css -c postcss.config.js -o public/css/app.css --w -m false",
        "dev": "concurrently --kill-others 'npm run start-server:dev' 'npm run start-dev' 'npm run browser-sync' 'npm run postcss'",
        "dev:lint": "concurrently --kill-others 'npm run start-server:dev' 'npm run start-dev:lint' 'npm run browser-sync' 'npm run postcss'",
        "start": "node -r babel-register bin/www"
    },
    "repository": {
        "type": "git"
    },
    "author": "clover.blue",
    "private": true,
    "engines": {
        "node": "~10"
    },
    "dependencies": {
        "body-parser": "~1.6.6",
        "classnames": "^2.2.5",
        "connect-mongo": "2.0.1",
        "connect-redis": "~2.1.0",
        "cookie-parser": "~1.3.2",
        "csurf": "1.5.0",
        "ejs": "~0.8.5",
        "express": "~4.8.6",
        "express-paginate": "1.0.0",
        "express-session": "~1.7.6",
        "firebase-admin": "^8.3.0",
        "lodash": "^4.17.4",
        "marked": "~0.4.0",
        "method-override": "~2.1.3",
        "moment": "2.18.1",
        "mongoose": "~5.2.9",
        "mongoose-paginate": "git+https://github.com/edwardhotchkiss/mongoose-paginate#ES6-6.0",
        "morgan": "~1.2.3",
        "qs": "^6.5.0",
        "react": "^16.4.2",
        "react-dom": "^16.4.2",
        "react-preload": "^0.2.3",
        "react-redux": "^4.4.2",
        "react-router": "^3.2.1",
        "react-router-redux": "^4.0.2",
        "redux": "^3.4.0",
        "redux-thunk": "^2.2.0",
        "request": "~2.49.0"
    },
    "devDependencies": {
        "@babel/register": "^7.5.5",
        "autoprefixer": "^9.1.3",
        "babel-core": "^6.25.0",
        "babel-eslint": "^7.2.2",
        "babel-loader": "^7.1.1",
        "babel-plugin-react-transform": "^2.0.2",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "6.24.1",
        "bluebird": "^3.4.6",
        "browser-sync": "^2.18.7",
        "concurrently": "3.5.0",
        "css-loader": "^1.0.0",
        "enzyme": "^2.7.1",
        "eslint": "^3.19.0",
        "eslint-config-airbnb": "^15.0.2",
        "eslint-loader": "^1.9.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jsx-a11y": "^5.1.1",
        "eslint-plugin-react": "^7.1.0",
        "mocha": "^3.2.0",
        "nodemon": "1.11.0",
        "postcss-cli": "^6.0.0",
        "postcss-import": "^10.0.0",
        "postcss-loader": "^3.0.0",
        "power-assert": "^1.4.2",
        "precss": "^3.1.2",
        "react-addons-test-utils": "^15.4.2",
        "react-hot-loader": "^1.3.1",
        "react-transform-catch-errors": "^1.0.2",
        "react-transform-hmr": "^1.0.4",
        "redbox-react": "^1.1.1",
        "style-loader": "^0.22.1",
        "webpack": "^3.0.0",
        "webpack-dev-middleware": "^1.11.0",
        "webpack-hot-middleware": "^2.18.2"
    }
}
